public class PairSum {
    public static int ArrayPairSum(int arr[],int x){
        int count=0;
        for(int i=0;i<arr.length;i++){
            for(int j=i+1;j<arr.length;j++){
                if(arr[i]+arr[j]==x) {
                    count = count + 1;
                }
            }
        }
        return count;
    }
}
