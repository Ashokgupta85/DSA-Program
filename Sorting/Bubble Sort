public class Solution {
    
    public static void bubbleSort(int[] arr, int n) {   
        // Write your code here.
       for(int i=0;i<n;i++) {
            int count = 0;
            for (int j = count + 1; j < n-i; j++) {
                if (arr[count] > arr[j]) {
                    int temp = arr[j];
                    arr[j] = arr[count];
                    arr[count] = temp;
                }
                count++;

            }
        }

   

    }

}
