public class findunique {

    public static int findUnique(int arr[]) {
        //Your code goes here
        int i = 0;
        for (i = 0; i < arr.length; i++) {
            int x = 0;
            for (int j = 0; j < arr.length; j++) {
                if (arr[i] == arr[j]) {
                    x++;
                }
            }
            if (x == 1) {
                return arr[i];
            }
        }
        return arr[i];
    }
//
