// main logic-> arr[(i+k)%size]=temp[i];

class Solution {
    public void rotate(int[] arr, int k) {
        int n=arr.length;
        int[] temp=new int[n];
       for(int i=0;i<n;i++){
          temp[i]=arr[i];
       }
        for(int i=0;i<n;i++){
       arr[(i+k)%n]=temp[i];
        }
        
}
}
