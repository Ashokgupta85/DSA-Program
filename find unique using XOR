public class Main
{
    public static int FindUniqueUsingXOR(int arr[]){
        int ans=arr[0];
        for(int i=1;i<arr.length;i++){
            ans=ans^arr[i];
        }
        return ans;
    }
	public static void main(String[] args) {
		int arr[]={2,5,3,2,3,5,6,7,6};
        
       int a= FindUniqueUsingXOR(arr);
    
      System.out.println(a);    
}}
